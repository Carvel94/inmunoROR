/*------------------*/
/*	MENU
/*------------------------------------------------------*/

.menu_main {
	float: left;
	width: 74%;
}

#access .menu {
	list-style: none;
	position: relative;
	float: right;
	font-family: 'Open Sans', sans-serif;
	font-weight: 450;
/*	text-transform: uppercase; */
	margin-top: 0px;
}


#access .menu select {
	border: 2px solid #eee;
	padding: 10px 10px 10px 10px;
	background-color: #f9f9f9;
	color: #333;
}
#access .menu ul {
	margin: 0px;
	list-style-type: none;
	position: relative;
	text-align: left;
	z-index: 1001;
	margin: 0px 0px 0px 0px;
	float: left;
	height: 50px;
}
#access .menu ul li {
	position: relative;
	/*display: inline-block;*/
	padding: 0;
	z-index: 101;
	margin: 0px 15px 0px 13px;
	float: left;
	text-align:center;

}

#access .menu ul li a {
	display: inline-block;
	text-decoration: none;
	font-family: 'Open Sans', sans-serif;
	font-size: 15px;
	color: #404040;
	margin: 0;
	padding: 3px 1px 19px 1px;
	text-align: center;
	height: 50px;
}


#access .menu ul li a:hover {
	color: #0896b8;
} 
#access .menu ul li a.active {
	color: #0896b8;
}
#access .menu ul li i {
	margin-left: 2px;
}

#access .menu ul li ul {
	position: absolute;
	left: 0;
	height: auto;
	display: none;
	visibility: hidden;
	width: 200px;
	padding: 0;
	margin-top: 0px;
	text-align: left;
	background: #0896b8;
	border-top-left-radius: 0px;
	-moz-box-shadow: 0px 0px 1px rgba(0, 0, 0, 0.4);
	-webkit-box-shadow: 0px 0px 1px rgba(0, 0, 0, 0.4);
	box-shadow: 0px 0px 1px rgba(0, 0, 0, 0.4);
}
#access .menu ul li ul li {
	display: list-item;
	float: none;
	background: none;
	padding: 0;
	margin: 0;
	height: auto;-0
}
#access .menu ul li ul li ul {
	top: 0;
}
#access .menu ul li ul li a {
	margin: 0;
	border: none;
	display: block;
	padding: 10px 17px 10px 8px;
	font-size: 14px;
	font-family: 'Open Sans', sans-serif;
	font-weight: 450;
	color: #fff;
	background: #0896b8;
	width: 200px;
	text-align: left;
	border-bottom: 1px solid #1ca8ca;
}
#access .menu ul li ul li a:hover,
#access .menu ul li ul li.active a,
#access .menu ul li ul li a.selected {
	background: #9ac63d;
	border-bottom: 1px solid #1ca8ca;
	color: #fff;
	padding: 10px 17px 10px 8px;
}

#access .menu ul li ul li:last-child {
	border: none;
}
* html #access .menu {
	height: 1%;
}

.selectnav {display: none; width: 440px; margin-left: 20px;}



