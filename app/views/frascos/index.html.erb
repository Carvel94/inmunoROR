<p id="notice"><%= notice %></p>

<h1>Listing Frascos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @frascos.each do |fra| %>
      <th><%= link_to "#{fra.fetch(:nomAp)}:", frasco_path(fra) %></td>
      <tr>
        <td><%= "Frasco #{fra.fetch(:frasco)}. #{fra.fetch(:ent)}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

